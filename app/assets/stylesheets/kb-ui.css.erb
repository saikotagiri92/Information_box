/* Lato Fonts */
body{
	
background-color: rgb(245,245,245) !important;

height: 100% !important;
font-family: "LatoWebSymbol",Lato,Verdana,sans-serif !important;
font-size: 14px !important;
font-weight: 400 !important;
padding-top: 40px !important;
height: 100% !important;
}



.fa-bars{
	padding-top: 11px !important;
}
/* Error messages */
#error_explanation {
  color: #f00;
  
}
.error-message{
	list-style: none;
    margin: 0 0 18px 20px;
}
.field_with_errors {
  @extend .control-group;
  @extend .error;
}

/* Flash Message */
.flash-message-alert{
	z-index: 2 !important; 
	position: fixed !important; 
	margin-bottom: 0px !important;
	margin-left: 20px !important;
	margin-top: 10px !important;
	box-shadow: 0px 6px 12px rgba(0, 0, 0, 0.176) !important;
}

/* User feedback */
.user-feedback-button {
	display: inline-block; 
	background-color: rgb(97, 136, 204); 
	color: white; 
	outline: 0; 
	border: 0;
	padding-left: 20px;
	padding-right: 20px;
	text-decoration: none !important; 
	white-space: nowrap; 
	cursor: pointer; 
	vertical-align: middle; 
	text-shadow: none;
	padding-top: 5px; 
	padding-bottom: 5px;
	font-size: 16px !important; 
	-ms-transform:rotate(270deg); /* IE 9 */
  	-moz-transform:rotate(270deg); /* Firefox */
  	-webkit-transform:rotate(270deg); /* Safari and Chrome */
  	-o-transform:rotate(270deg); /* Opera */
	margin-top: 25% !important;
	position: fixed !important;
	border-radius: 3px;
	right: -37px;
	cursor: pointer;
	z-index: 1;
}
.application-bug-raise-button {
	display: inline-block; 
	background-color: rgb(97, 136, 204); 
	color: white; 
	outline: 0; 
	border: 0;
	padding-left: 20px;
	padding-right: 20px;
	text-decoration: none !important; 
	white-space: nowrap; 
	cursor: pointer; 
	vertical-align: middle; 
	text-shadow: none;
	padding-top: 5px; 
	padding-bottom: 5px;
	font-size: 16px !important; 
	bottom: 0px !important;
	position: fixed;
	border-radius: 2px 2px 0 0 !important;
	right: 0px !important;
	cursor: pointer;
	z-index: 1;
}
.application-bug-raise-button:hover{
	color: white !important;
	background-color: rgb(91, 131, 200);
}
.application-bug-raise-button:focus{
	color: white !important;
}
.user-feedback-button:hover{
	color: white !important;
	background-color: rgb(91, 131, 200);
}
.user-feedback-button:focus{
	color: white !important;
}
.user-feedback-textarea{
	resize: none !important;
}
.user-feedback-modal{
	width: 500px !important;
	height: 354px;
}
/* Article side bar new */
.new-article-sidebar{
	width: 15% !important;
	position: fixed !important;
	float: left !important;
	top: 0;
	left: 0;
	bottom: 0;
	border: 1px solid #ccc;
	border-width: 0 1px 0 0;  
	background-color: #f2f2f2 !important;
	overflow: hidden;
	height: 100% !important;
	
}
.article-sidebar-content{
	margin-top:20px !important;
	position: fixed !important;
	padding-left: 15px !important;
	width: 15%;
	height: 93% !important;
}
.sidebar-box-name{
	font-size: 18px;
	font-weight:bold;
	margin: 25px 5px 5px 0;
	color: #454f53;
}

.sidebar-box-description{
	text-decoration: none;
	color:#848484 !important; 
}

.sidebar-new-article{
	font-size: 16px !important;
	margin-top: 10px !important;
	margin-bottom: 10px !important;
}

.sidebar-article-tree{
	height: 72% !important;
	overflow: auto !important; 
}
.article-sidebar-box-settings{
	width: 15%; 
	border: 1px #ccc;
	bottom: 0;
	height: 40px !important;
	margin-bottom: 0;
	z-index: 0;
	position: fixed;
	padding-bottom: 10px;
}

.box-settings-link{
	padding-left: 17px;
	bottom: 0px;
	font-size: 12px;
	font-weight: bolder;
}
/*Article new side bar style ends */
/* Email Verification */
.email-verification-form-panel{
	margin-left: -15px !important;
}
/* Article show page */
.article-show-page{
	margin-top: 40px!important;
	top: 0px !important; 
	bottom: 0px !important;
	overflow-y: auto !important;
	width: 100% !important;
	background-color: white !important;
	height: 100% !important;
}

.article-show-content{
	float: right !important; 
	width: 85% !important;
	overflow-x: hidden !important; 
}
.article-breadcrumb{
	font-size: 14px !important;
	margin-top: 10px !important;
	margin-left: -10px !important;
	margin-bottom: -12px !important;
}


.article-post-meta{
	font-size: 12px;
	margin-top: 5px !important;
	margin-bottom: 15px !important;
	margin-left: 5px !important;
}
.article-options{
	margin-top: 10px !important;
}
.article-title{
	font-size: 30px !important;
	margin: 10px 0 20px -25px !important;
	width: 400px;
	font-weight: bold !important;
}
#article-content{
	margin-bottom: 45px; 
	margin-left: 3px !important;
	padding-left: -4px !important;
}
