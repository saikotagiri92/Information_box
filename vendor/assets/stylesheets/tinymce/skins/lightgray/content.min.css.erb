#tinymce h1{
	font-size: 24px !important;
	font-weight: bold !important;
	margin-bottom: 20px !important;
	
}

#tinymce a{
	color: rgb(66, 139, 202) !important;
text-decoration: none !important;
}
#tinymce h1 > a{
	font-size: 24px !important;
	font-weight: bold !important;
	margin-bottom: 20px !important;
	margin-left: -5px !important;
}

#tinymce h2 > a {
	font-size: 20px !important;
	font-weight: bold !important;
	margin-bottom: 20px !important;
}

#tinymce h3 > a {
	font-size: 18px !important;
	font-weight: bold !important;
	margin-bottom: 20px !important;
}

#tinymce h4 > a {
	font-size: 14px !important;
	font-weight: bold !important;
	margin-bottom: 20px !important;
}

#tinymce h5 > a {
	font-size: 12px !important;
	font-weight: bold !important;
	margin-bottom: 20px !important;
}

#tinymce h6 > a {
	font-size: 12px !important;
	font-weight: bold !important;
	margin-bottom: 20px !important;
}

/* blockqoute styling */

#tinymce blockquote{
	font-size: 14px !important;
}

/* Subscript & Superscript styling */
#tinymce sub, sup{
	font-size: 14px !important;
}

/* bold, italcs & underline styling */
#tinymce strong{
	font-weight: bold;
}
#tinymce em{
	font-style: italic;
}

/* paragraph styling */
#tinymce p {
  font-size: 14px !important;
  margin-bottom: 20px !important;
}

#tinymce h2{
	font-size: 20px !important;
	font-weight: bold !important;
	margin-bottom: 20px !important;
}

#tinymce h3{
	font-size: 18px !important;
	font-weight: bold !important;
	margin-bottom: 20px !important;
}

#tinymce h4{
	font-size: 14px !important;
	font-weight: bold !important;
	margin-bottom: 20px !important;
}

#tinymce h5{
	font-size: 12px !important;
	font-weight: bold !important;
	margin-bottom: 20px !important;
}

#tinymce h6{
	font-size: 12px !important;
	font-weight: bold !important;
	margin-bottom: 20px !important;
}

#tinymce strong{
	font-size: 14px !important;
	font-weight: bold !important;
}

li#inline-task-checked{
    list-style: none outside none !important;
    padding-left: 18px !important;
    color: rgb(112, 112, 112) !important;
    background-repeat: no-repeat !important;
    background-size: 16px 16px !important;
    background-image:url('<%= asset_path 'checkbox-checked.png' %>') !important;
}

li#inline-task-checked a{
    color: rgb(112, 112, 112) !important;
    text-decoration: underline !important; 
    
}
#tinymce{
	background-color:#FFFFFF;
	color: #353535 !important;
	font-family: "LatoWebSymbol",Lato,Verdana,sans-serif !important;
	font-size: 14px !important;	margin-left: 1% !important;
	scrollbar-3dlight-color:#F0F0EE;
	scrollbar-arrow-color:#676662;
	scrollbar-base-color:#F0F0EE;
	scrollbar-darkshadow-color:#DDDDDD;
	scrollbar-face-color:#E0E0DD;
	scrollbar-highlight-color:#F0F0EE;
	scrollbar-shadow-color:#F0F0EE;
	scrollbar-track-color:#F5F5F5
}


#tinymce em{
	font-style: italic !important;
}
.mce-object{
	border:1px dotted #3A3A3A;
	background:#d5d5d5 url(img/object.gif) no-repeat center
}

.mce-pagebreak{
	cursor:default;
	display:block;
	border:0;
	width:100%;
	height:5px;
	border:1px dashed #666;
	margin-top:15px
}

.mce-item-anchor{
	cursor:default;
	display:inline-block;
	-webkit-user-select:all;
	-webkit-user-modify:read-only;
	-moz-user-select:all;
	-moz-user-modify:read-only;
	user-select:all;
	user-modify:read-only;
	width:9px !important;
	height:9px !important;
	border:1px dotted #3A3A3A;
	background:#d5d5d5 url(img/anchor.gif) no-repeat center
}

.mce-nbsp{
	background:#AAA
}

hr{
	cursor:default
}

.mce-match-marker{
	background:#AAA;
	color:#fff
}

.mce-match-marker-selected{
	background:#3399ff;
	color:#fff
}

.mce-spellchecker-word{
	background:url(img/wline.gif) repeat-x bottom left;
	cursor:default
}

/* Table Styling */
#tinymce table{
	overflow-x: auto;
	border-spacing: 0px !important;
	border-collapse: 0px !important;
}
#tinymce table tr:first-child td{
	background-color: #efefef !important;
	font-weight: bold !important;
}
#tinymce table{
	margin: 10px !important;
	border: 1px #dddddd solid !important;
	text-align: center !important;

}

#tinymce table > tr > th{
	border: 1px #dddddd solid;
	text-align: center !important;

}

#tinymce table > tr > td{
	text-align: center !important;

}
#tinymce td {
	min-width: 20px !important;
	border: 1px #dddddd solid;
	text-align: center !important;	

}


li#inline-task-unchecked{
    list-style: none outside none !important;
    padding-left: 18px !important;
    
    background-repeat: no-repeat !important;
    background-size: 16px 16px !important;
    background-image:url('<%= asset_path 'checkbox-unchecked.png' %>') !important;
}

.mce-edit-focus{
	outline:1px dotted #333
}
/* List Styling */
#tinymce ul {
  padding-left: 0px !important;
  list-style: disc;
  margin-left: 1% !important;
}

#tinymce ol {
	padding-left: 0px !important;
 	margin-left: 1% !important;
 	list-style: decimal; 
}

#tinymce > ul >  li {
  
  margin-left: 1.5% !important;
  font-size: 14px !important;
}
#tinymce > ol >  li {
  
  margin-left: 1.5% !important;
  font-size: 14px !important;
}
#tinymce ul li li{
	margin-left: 35px !important;
	list-style: square;

}

#tinymce ul li li li{
	list-style: circle;
}


#tinymce ol li li{
	margin-left: 35px !important;
	list-style: lower-alpha;
}

#tinymce ol li li li{
	list-style: lower-roman;

}